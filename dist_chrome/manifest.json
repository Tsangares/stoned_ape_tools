{
  
    "name": "Stoned Ape Tools",
    "description": "Tools that only a stoned ape could use. ",
    "version": "2.29.2",
    "manifest_version": 3,  
    "permissions": [
      "scripting"
    ],
    "host_permissions": [
      "https://np.ironhelmet.com/*"
    ],
    "action": {},
    "web_accessible_resources": [
      {
        "resources": [
          "intel.js",
          "*.png"
        ],
        "matches": [
          "<all_urls>"
        ]
      }
    ],
    "content_scripts": [
      {
        "matches": [ "https://np.ironhelmet.com/*" ],
        "css": [ "intel.css" ]
      }
    ],
    "background": {
      "service_worker": "service_worker.js"
    },
  
    "icons": {
      "16": "favicon.png",
      "48": "favicon.png",
      "128": "favicon.png"
    }
  }
  
