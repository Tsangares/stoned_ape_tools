{
  "name": "Stoned Ape Tools",
  "description": "Tools that only a stoned ape could use. ",
  "version": "2.29.7",
  "manifest_version": 3,
  "browser_specific_settings": {
    "gecko": {
      "id": "{1a428aba-9fe5-4388-af4a-dc57b5974284}"
    }
  },
  "permissions": [
    "scripting"
  ],
  "host_permissions": [
    "https://np.ironhelmet.com/*"
  ],
  "action": {},
  "web_accessible_resources": [
    {
      "resources": [
        "intel.js",
        "*.png"
      ],
      "matches": [
        "<all_urls>"
      ]
    }
  ],
  "content_scripts": [
    {
      "matches": [
        "https://np.ironhelmet.com/*"
      ],
      "css": [
        "intel.css"
      ]
    }
  ],
  "background": {
    "scripts": [
      "worker.js"
    ]
  },
  "icons": {
    "16": "favicon.png",
    "48": "favicon.png",
    "128": "favicon.png"
  }
}